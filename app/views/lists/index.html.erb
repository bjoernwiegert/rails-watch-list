 <h1>Save any kind of movies</h1>

<h3>My Lists</h3>
<div class="row">
  <% @lists.each do |list| %>
    <div>
      <%= link_to list_path(list) do %>
        <%= list.name %>
      <% end %>
    </div>
  <% end %>
</div>
<%= link_to "Add a new list", new_list_path, class: 'btn btn-primary mt-3' %>
